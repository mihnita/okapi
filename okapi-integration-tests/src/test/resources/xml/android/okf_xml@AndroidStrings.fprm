<?xml version="1.0" encoding="UTF-8" standalone="no"?><its:rules xmlns:its="http://www.w3.org/2005/11/its" xmlns:a="http://schemas.android.com/apk/res/android" version="1.0">
 <its:translateRule selector="/*" translate="no"/>
 <its:translateRule selector="//*/@a:text[not(starts-with(.,'@'))]" translate="yes"/>
 <its:translateRule selector="//string|//item" translate="yes"/>
 <its:withinTextRule selector="//string/descendant::*" withinText="yes"/>
</its:rules>