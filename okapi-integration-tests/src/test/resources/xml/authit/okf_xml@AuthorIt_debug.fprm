<?xml version="1.0" encoding="UTF-8" standalone="no"?><its:rules xmlns:its="http://www.w3.org/2005/11/its" xmlns:aut="http://www.authorit.com/xml/authorit" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:itsx="http://www.w3.org/2008/12/its-extensions" xmlns:okp="okapi-framework:xmlfilter-options" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.0">
<!-- See ITS specification at: http://www.w3.org/TR/its/ -->
<its:withinTextRule selector="//aut:tref " withinText="yes" itsx:whiteSpaces="preserve"/>

<its:translateRule selector="//aut:tref " translate="no" itsx:whiteSpaces="preserve"/>
<its:translateRule selector="//aut:All | //aut:p " translate="yes" itsx:whiteSpaces="preserve"/>

<its:translateRule selector="//aut:Object/descendant-or-self::* " translate="no" itsx:whiteSpaces="preserve"/>

<okp:options escapeQuotes="no" extractIfOnlyCodes="yes" />

</its:rules>