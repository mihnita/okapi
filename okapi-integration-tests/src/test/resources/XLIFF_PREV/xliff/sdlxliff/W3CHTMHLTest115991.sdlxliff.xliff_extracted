<trans-unit id="fb5574cd-e1b9-4f27-8053-c5b0d2f6c662" translate="no">
<source xml:lang="en-us"></source>
<target xml:lang="es-es"></target>
</trans-unit>
<trans-unit id="b712c0e0-72d2-4e4c-9079-995f85d0f68b">
<source xml:lang="en-us"><it id="247" ctype="x-empty" pos="open">&lt;strong&gt;</it>This section is informative.<it id="247" ctype="x-empty" pos="close">&lt;/strong&gt;</it></source>
<seg-source><it id="247" ctype="x-empty" pos="open">&lt;strong&gt;</it><mrk mid="95" mtype="seg">This section is informative.</mrk><it id="247" ctype="x-empty" pos="close">&lt;/strong&gt;</it></seg-source>
<target xml:lang="es-es"><it id="247" ctype="x-empty" pos="open">&lt;strong&gt;</it><mrk mid="95" mtype="seg">This section is informative.</mrk><it id="247" ctype="x-empty" pos="close">&lt;/strong&gt;</it></target>
</trans-unit>
<trans-unit id="4028ba6d-95d8-4dfe-b455-92b16acb9bea" translate="no">
<source xml:lang="en-us"><ph id="248" ctype="x-empty">&lt;/p&gt;</ph></source>
<target xml:lang="es-es"><ph id="248" ctype="x-empty">&lt;/p&gt;</ph></target>
</trans-unit>
