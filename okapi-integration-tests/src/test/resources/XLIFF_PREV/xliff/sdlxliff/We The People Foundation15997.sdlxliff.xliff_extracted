<trans-unit id="103bc679-788c-4416-8a42-226c25eb231a">
<source xml:lang="en-us"><bpt id="4119" ctype="x-empty">&lt;font size="1" face="Arial"&gt;</bpt><ph id="4120" ctype="x-empty">&lt;br&gt;</ph> <ept id="4119">&lt;/font&gt;</ept><bpt id="4119" ctype="x-empty">&lt;font size="1" face="Arial"&gt;</bpt>April 7, 2007<ept id="4119">&lt;/font&gt;</ept><ph id="4121" ctype="x-empty">&lt;br&gt;</ph> <ph id="4122" ctype="x-empty">&lt;br&gt;</ph> <bpt id="4123" ctype="x-empty">&lt;b&gt;</bpt><bpt id="4124" ctype="x-empty">&lt;span style="font-family: Arial; color: rgb(0, 0, 128);"&gt;</bpt><bpt id="4125" ctype="x-empty">&lt;font size="4"&gt;</bpt>U.S., Israel and AIPAC:<ept id="4125">&lt;/font&gt;</ept><ept id="4124">&lt;/span&gt;</ept><ept id="4123">&lt;/b&gt;</ept><it id="4123" ctype="x-empty" pos="open">&lt;b&gt;</it><bpt id="4124" ctype="x-empty">&lt;span style="font-family: Arial; color: rgb(0, 0, 128);"&gt;</bpt><bpt id="4125" ctype="x-empty">&lt;font size="4"&gt;</bpt> <ept id="4125">&lt;/font&gt;</ept><ept id="4124">&lt;/span&gt;</ept><bpt id="4124" ctype="x-empty">&lt;span style="font-family: Arial; color: rgb(0, 0, 128);"&gt;</bpt><bpt id="4125" ctype="x-empty">&lt;font size="4"&gt;</bpt>“No, Thank You”<ept id="4125">&lt;/font&gt;</ept><ept id="4124">&lt;/span&gt;</ept><bpt id="4126" ctype="x-color:#000080;">&lt;font size="4" color="#000080"&gt;</bpt><bpt id="4127" ctype="x-empty">&lt;span style="font-family: Arial;"&gt;</bpt><ph id="4128" ctype="x-empty">&lt;br&gt;</ph> <ph id="4129" ctype="x-empty">&lt;br&gt;</ph> Palestine:<ept id="4127">&lt;/span&gt;</ept><ept id="4126">&lt;/font&gt;</ept><it id="4126" ctype="x-color:#000080;" pos="open">&lt;font size="4" color="#000080"&gt;</it><it id="4127" ctype="x-empty" pos="open">&lt;span style="font-family: Arial;"&gt;</it> “Yes, Thank You” <it id="4127" ctype="x-empty" pos="close">&lt;/span&gt;</it><it id="4126" ctype="x-color:#000080;" pos="close">&lt;/font&gt;</it><it id="4123" ctype="x-empty" pos="close">&lt;/b&gt;</it></source>
<seg-source><bpt id="4119" ctype="x-empty">&lt;font size="1" face="Arial"&gt;</bpt><ph id="4120" ctype="x-empty">&lt;br&gt;</ph> <ept id="4119">&lt;/font&gt;</ept><mrk mid="618" mtype="seg"><bpt id="4119" ctype="x-empty">&lt;font size="1" face="Arial"&gt;</bpt>April 7, 2007<ept id="4119">&lt;/font&gt;</ept><ph id="4121" ctype="x-empty">&lt;br&gt;</ph> <ph id="4122" ctype="x-empty">&lt;br&gt;</ph> <bpt id="4123" ctype="x-empty">&lt;b&gt;</bpt><bpt id="4124" ctype="x-empty">&lt;span style="font-family: Arial; color: rgb(0, 0, 128);"&gt;</bpt><bpt id="4125" ctype="x-empty">&lt;font size="4"&gt;</bpt>U.S., Israel and AIPAC:<ept id="4125">&lt;/font&gt;</ept><ept id="4124">&lt;/span&gt;</ept><ept id="4123">&lt;/b&gt;</ept></mrk><it id="4123" ctype="x-empty" pos="open">&lt;b&gt;</it><bpt id="4124" ctype="x-empty">&lt;span style="font-family: Arial; color: rgb(0, 0, 128);"&gt;</bpt><bpt id="4125" ctype="x-empty">&lt;font size="4"&gt;</bpt> <ept id="4125">&lt;/font&gt;</ept><ept id="4124">&lt;/span&gt;</ept><mrk mid="619" mtype="seg"><bpt id="4124" ctype="x-empty">&lt;span style="font-family: Arial; color: rgb(0, 0, 128);"&gt;</bpt><bpt id="4125" ctype="x-empty">&lt;font size="4"&gt;</bpt>“No, Thank You”<ept id="4125">&lt;/font&gt;</ept><ept id="4124">&lt;/span&gt;</ept><bpt id="4126" ctype="x-color:#000080;">&lt;font size="4" color="#000080"&gt;</bpt><bpt id="4127" ctype="x-empty">&lt;span style="font-family: Arial;"&gt;</bpt><ph id="4128" ctype="x-empty">&lt;br&gt;</ph> <ph id="4129" ctype="x-empty">&lt;br&gt;</ph> Palestine:<ept id="4127">&lt;/span&gt;</ept><ept id="4126">&lt;/font&gt;</ept></mrk><it id="4126" ctype="x-color:#000080;" pos="open">&lt;font size="4" color="#000080"&gt;</it><it id="4127" ctype="x-empty" pos="open">&lt;span style="font-family: Arial;"&gt;</it> <mrk mid="620" mtype="seg">“Yes, Thank You”</mrk> <it id="4127" ctype="x-empty" pos="close">&lt;/span&gt;</it><it id="4126" ctype="x-color:#000080;" pos="close">&lt;/font&gt;</it><it id="4123" ctype="x-empty" pos="close">&lt;/b&gt;</it></seg-source>
