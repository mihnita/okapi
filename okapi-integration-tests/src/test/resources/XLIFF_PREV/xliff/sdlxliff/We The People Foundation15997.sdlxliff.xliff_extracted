<trans-unit id="7d08c68b-dff4-4586-b93b-b3f7c814f192">
<source xml:lang="en-us"><bpt id="2550" ctype="x-empty">&lt;span style="font-size: 10pt; line-height: 115%; font-family: Arial;"&gt;</bpt>June 30, 2008<ept id="2550">&lt;/span&gt;</ept><bpt id="2551" ctype="x-empty">&lt;span style="font-size: 10pt; line-height: 115%; font-family: Arial;"&gt;</bpt> may go down in history as the day Americans began, in earnest,<ph id="2552" ctype="x-empty">&lt;br&gt;</ph> the moral and solemn process of holding their Government accountable to the Constitution by directly exercising their unalienable and individual Right of<ph id="2553" ctype="x-empty">&lt;br&gt;</ph> Redress, rather than depending upon the will of the majority of votes as cast<ph id="2554" ctype="x-empty">&lt;br&gt;</ph> in local precincts, the halls of Congress or the inner chambers of our<ph id="2555" ctype="x-empty">&lt;br&gt;</ph> courthouses.<ept id="2551">&lt;/span&gt;</ept><it id="2551" ctype="x-empty" pos="open">&lt;span style="font-size: 10pt; line-height: 115%; font-family: Arial;"&gt;</it>  <it id="2556" ctype="x-color:#000080;" pos="open">&lt;font color="#000080"&gt;</it><it id="2557" ctype="x-empty" pos="open">&lt;b&gt;</it><it id="2558" ctype="x-empty" pos="open">&lt;i&gt;</it>(continued...)<it id="2558" ctype="x-empty" pos="close">&lt;/i&gt;</it><it id="2557" ctype="x-empty" pos="close">&lt;/b&gt;</it><it id="2556" ctype="x-color:#000080;" pos="close">&lt;/font&gt;</it><it id="2551" ctype="x-empty" pos="close">&lt;/span&gt;</it></source>
<seg-source><mrk mid="377" mtype="seg"><bpt id="2550" ctype="x-empty">&lt;span style="font-size: 10pt; line-height: 115%; font-family: Arial;"&gt;</bpt>June 30, 2008<ept id="2550">&lt;/span&gt;</ept><bpt id="2551" ctype="x-empty">&lt;span style="font-size: 10pt; line-height: 115%; font-family: Arial;"&gt;</bpt> may go down in history as the day Americans began, in earnest,<ph id="2552" ctype="x-empty">&lt;br&gt;</ph> the moral and solemn process of holding their Government accountable to the Constitution by directly exercising their unalienable and individual Right of<ph id="2553" ctype="x-empty">&lt;br&gt;</ph> Redress, rather than depending upon the will of the majority of votes as cast<ph id="2554" ctype="x-empty">&lt;br&gt;</ph> in local precincts, the halls of Congress or the inner chambers of our<ph id="2555" ctype="x-empty">&lt;br&gt;</ph> courthouses.<ept id="2551">&lt;/span&gt;</ept></mrk><it id="2551" ctype="x-empty" pos="open">&lt;span style="font-size: 10pt; line-height: 115%; font-family: Arial;"&gt;</it>  <it id="2556" ctype="x-color:#000080;" pos="open">&lt;font color="#000080"&gt;</it><it id="2557" ctype="x-empty" pos="open">&lt;b&gt;</it><it id="2558" ctype="x-empty" pos="open">&lt;i&gt;</it><mrk mid="378" mtype="seg">(continued...)</mrk><it id="2558" ctype="x-empty" pos="close">&lt;/i&gt;</it><it id="2557" ctype="x-empty" pos="close">&lt;/b&gt;</it><it id="2556" ctype="x-color:#000080;" pos="close">&lt;/font&gt;</it><it id="2551" ctype="x-empty" pos="close">&lt;/span&gt;</it></seg-source>
<target xml:lang="es-es"><mrk mid="377" mtype="seg"><bpt id="2550" ctype="x-empty">&lt;span style="font-size: 10pt; line-height: 115%; font-family: Arial;"&gt;</bpt><ept id="2550">&lt;/span&gt;</ept><bpt id="2551" ctype="x-empty">&lt;span style="font-size: 10pt; line-height: 115%; font-family: Arial;"&gt;</bpt><ph id="2552" ctype="x-empty">&lt;br&gt;</ph><ph id="2553" ctype="x-empty">&lt;br&gt;</ph><ph id="2554" ctype="x-empty">&lt;br&gt;</ph><ph id="2555" ctype="x-empty">&lt;br&gt;</ph><ept id="2551">&lt;/span&gt;</ept></mrk><it id="2551" ctype="x-empty" pos="open">&lt;span style="font-size: 10pt; line-height: 115%; font-family: Arial;"&gt;</it>  <it id="2556" ctype="x-color:#000080;" pos="open">&lt;font color="#000080"&gt;</it><it id="2557" ctype="x-empty" pos="open">&lt;b&gt;</it><it id="2558" ctype="x-empty" pos="open">&lt;i&gt;</it><mrk mid="378" mtype="seg"></mrk><it id="2558" ctype="x-empty" pos="close">&lt;/i&gt;</it><it id="2557" ctype="x-empty" pos="close">&lt;/b&gt;</it><it id="2556" ctype="x-color:#000080;" pos="close">&lt;/font&gt;</it><it id="2551" ctype="x-empty" pos="close">&lt;/span&gt;</it></target>
