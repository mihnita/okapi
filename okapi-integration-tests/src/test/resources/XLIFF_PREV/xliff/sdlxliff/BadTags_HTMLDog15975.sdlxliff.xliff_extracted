<group id="g78">
<trans-unit id="68973ace-e5f7-4244-8662-ca130f8d86eb" translate="no">
<source xml:lang="en-us"><ph id="187" ctype="x-empty">&lt;li&gt;</ph></source>
<target xml:lang="es-es"><ph id="187" ctype="x-empty">&lt;li&gt;</ph></target>
</trans-unit>
